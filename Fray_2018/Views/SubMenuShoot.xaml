<UserControl x:Class="Fray_2018.Views.SubMenuShoot"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Fray_2018.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    
        
    <StackPanel DataContext="{Binding SubMenuShootViewModel, Source={StaticResource FrayLocator}}" Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center">
        <Button Content="Fray" Command="{Binding Path=FrayMenuCommand}" />
        <Button Content="Portrait" Command="{Binding Path=PortraitMenuCommand}"/>
        <Button Content="UV" Command="{Binding Path=UVMenuCommand}"/>
    </StackPanel>
    
</UserControl>
